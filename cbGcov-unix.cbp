<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="cbGcov (unix)" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel/share/codeblocks/plugins/cbGcov" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs/plugins/contrib/cbGcov" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log -na -ns -nd" />
				<Option host_application="../../../devel/codeblocks" />
				<Compiler>
					<Add option="-g" />
					<Add option="`pkg-config --cflags codeblocks`" />
					<Add option="`wx-config --cflags`" />
					<Add option="-Ulinux" />
					<Add option="-Uunix" />
					<Add option="-Winvalid-pch" />
					<Add option="-fexceptions" />
					<Add option="-fPIC" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="../../../include" />
					<Add directory="../../../sdk/wxscintilla/include" />
				</Compiler>
				<Linker>
					<Add option="`pkg-config --libs codeblocks`" />
					<Add option="`wx-config --libs`" />
					<Add directory="../../../devel" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ../../../devel/share/codeblocks/cbGcov.zip manifest.xml" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Unit filename="GcovProcess.cpp" />
		<Unit filename="GcovProcess.h" />
		<Unit filename="cbGcov.cpp" />
		<Unit filename="cbGcov.h" />
		<Unit filename="cbGcovConfigPanel.cpp" />
		<Unit filename="cbGcovConfigPanel.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="wxsmith/cbGcovConfigPanel.wxs" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
				<resources>
					<wxPanel wxs="wxsmith/cbGcovConfigPanel.wxs" src="cbGcovConfigPanel.cpp" hdr="cbGcovConfigPanel.h" fwddecl="1" i18n="1" name="cbGcovConfigPanel" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
